<!DOCTYPE html>
<html>
  <head>
    <title><%= obj.title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <style type="text/css">
        body > nav, body > section {
            float: left;
            height : 200px;
        }
        .users{
            float: left;
        }
        nav{
            background: lightgreen;
        }
        header{
            background: lightblue;
        }
        section{
            background: lightpink;
        }
        section > article{
            background: lightgray;
            margin: 5px 5px 5px 5px;
        }
        table td {
            border : solid 1px;
        }
    </style>
    <script src="../js/axios.min.js"></script>
    <script>
        var nav_menu = function(){
            axios.get('./nav.ejs')
            .then((response) => {
                document.querySelector("#nav_menu").innerHTML = response.data;
            })
            .catch((err) => {
                console.log(err);
            });
        }
        nav_menu();
        // function ss(){
        //     document.getElementById('#nav_menu').load('./nav.ejs');
        // }
        // ss();
    </script>
  </head>
  <body>
        <nav id="nav_menu">
        </nav>
      <section>
        <header>
            <!-- <%= obj.title %> -->
        </header>
        <section>
            <article>
                <div>
                    <p>
                        <span>
                            연도 : <select name="" id="">
                                <% if (obj.year) { %>
                                    <option value=<%= obj.year[0] %> selected><%= obj.year[0] %></option>
                                    <option value=<%= obj.year[1] %>><%= obj.year[1] %></option>
                                    <option value=<%= obj.year[2] %>><%= obj.year[2] %></option>
                                <% } %>
                            </select></span>
                        <span>학기 : 
                            <select name="" id="">
                                <% if (obj.year) { %>
                                    <option value='1' selected>1</option>
                                    <option value='2'>2</option>
                                <% } %>
                            </select>
                        </span> <button>검색</button>
                    </p>
                    <p>
                        <span>
                            <select name="" id="">
                                    <option value="subject" selected>과목명</option>
                                    <option value="cource">강좌명</option>
                                    <option value="professor">교육자명</option>
                            </select>
                        </span>
                        <input type="text" name="" id="" placeholder="검색어를 입력해주세요."> <button>검색</button>
                    </p>
                </div>
            </article>
            <article>
                    <table>
                            <thead>
                                <td>
                                    연도
                                </td>
                                <td>
                                    학기
                                </td>
                                <td>
                                    과목
                                </td>
                                <td>
                                    강좌
                                </td>
                                <td>
                                    교육자명
                                </td>
                                <td>
                                    중간점수
                                </td>
                                <td>
                                    기말점수
                                </td>
                            </thead>
                            <tr>
