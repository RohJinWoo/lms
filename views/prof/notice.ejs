<!DOCTYPE html>
<html>
  <head>
    <title><%= obj !== undefined ? obj.title : '' %></title>

    <link href="../resume/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet">
    <link href="../resume/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
  
    <!-- Custom styles for this template -->
    <link href="../resume/css/resume.min.css" rel="stylesheet">
  
    <link rel="stylesheet" href="../stylesheets/uploadfile_input.css">
  </head>
  <body>
    <h2 class="mb-5"><a href="/prof/main" class="backlink">&lt;&nbsp;</a><%= obj !== undefined ? obj.title : '' %></h2>
    <form id="notice">
      <div class="board" id ="board"></div>
      <div class="pagination"></div>
    </form>
    <form id="form" onsubmit="return subclose();">
      <div id="notice_pagination" class="pagination_div">
      </div>
      <div class="input-group">
        <div class="input-group-btn">
          <button type="button" class="group-button dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
            <span class="caret" id="search_condition">제목</span>
          </button>
        <ul class="dropdown-menu" role="menu">
          <li name="search_condition_list" style="cursor: pointer;text-align: center"><a>제목</a></li>
          <li name="search_condition_list" style="cursor: pointer;text-align: center"><a>내용</a></li>
        </ul>
        </div>
        <input type="text" placeholder="검색어 입력" class="group-input" id="search_input">
        <span class="group-span">
          <button class="group-button" id="search">검색</button>
        </span>
      </div>
      <div class="notice_edit">
        <a href="./notice/edit">글쓰기</a>
      </div>
      <div id="next_search" class="button-group" style="text-align: center"></div>
    </form>
  </body>
  <script src = '../javascripts/util.js'></script>
  <script src = '../js/axios.min.js'></script>
  <script src = '../javascripts/notice_pagination.js'></script>

    <!-- Bootstrap core JavaScript -->
    <script src="../resume/vendor/jquery/jquery.min.js"></script>
    <script src="../resume/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  
    <!-- Plugin JavaScript -->
    <script src="../resume/vendor/jquery-easing/jquery.easing.min.js"></script>
    
    <script src="../javascripts/users/notice_test.js"></script>
    <script src="../javascripts/notice_search.js"></script>
  
</html>